[run]
branch = true
parallel = true
omit =
    .tox/*
    Fake_filename
    *console.py
    */_attribute_getter.py

[:report]
skip_covered = True
show_missing = True
exclude_lines =
    pragma: no cover
    def __repr__
    if self\.debug
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
